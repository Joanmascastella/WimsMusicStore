<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.inholland.nl.wimsmusicstore.Controller.DashboardController"
      prefHeight="400.0" prefWidth="600.0"
      spacing="10"
      style="-fx-padding: 15px;">

    <Label fx:id="userName"
           style="-fx-font-size: 24px; -fx-padding: 10px;">
    </Label>

    <Label fx:id="userRole"
           style="-fx-font-size: 16px; -fx-padding: 10px;">
    </Label>

    <Label fx:id="dateAndTime"
           style="-fx-font-size: 16px; -fx-padding: 10px;">
    </Label>

</VBox>
